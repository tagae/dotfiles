# ~/.shrc: sh(1) configuration

# If not running interactively, don't do anything
[ -z "$PS1" ] && return

# Load auxiliary functions
if [ -r ~/.sh.d/ancillary* ]; then
    source ~/.sh.d/ancillary* && load ~/.sh.d/interactive
    true # Assume success
else
    echo "Could not find ~/.sh.d framework." >&2
    exit 1
fi
